{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=AfroMarket;Integrated Security=true;TrustServerCertificate=true;"
  },
  "Keycloak": {
    "Realm": "afromarket",
    "AuthServerUrl": "http://localhost:8080",
    "SslRequired": "none",
    "Resource": "afromarket-merchant-service",
    "VerifyTokenAudience": true,
    "Credentials": {
      "Secret": ""
    }
  },
  "Cors": {
    "AllowedOrigins": "http://localhost:3000"
  }
}
